  "devDependencies": {
    "@remotion/eslint-config-flat": "4.0.327",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "19.0.0",
    "@types/react-dom": "^19.1.6",
    "@types/web": "0.0.166",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "9.19.0",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
