    "@remotion/renderer": "4.0.327",
    "@remotion/tailwind-v4": "4.0.327",
    "@remotion/transitions": "4.0.327",
    "@remotion/zod-types": "4.0.327",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
